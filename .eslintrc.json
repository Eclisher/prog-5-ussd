{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-case-declarations": "error",
      "no-unused-vars": "warn",
      "no-undef": "error"
    },
    "globals": {
        "process": "true"
    }
  }